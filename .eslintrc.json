{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "jasmine": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "react/jsx-one-expression-per-line": "off"
    }
}
